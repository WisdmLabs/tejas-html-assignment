{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wdm-plugins/fancy-header",
  "title": "Fancy Header",
  "category": "text",
  "description": "Adds a header with a hover effect",
  "keywords": [
    "header",
    "underline",
    "hover"
  ],
  "version": "1",
  "textdomain": "wdm-plugins",
  "editorScript": "file:./index.js",
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "h2"
    },
    "underline_color": {
      "type": "string",
      "default": "#f87171"
    }
  },
  "style": "file:./index.css"
}